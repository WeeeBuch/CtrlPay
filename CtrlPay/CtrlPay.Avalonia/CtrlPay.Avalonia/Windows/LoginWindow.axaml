<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="using:CtrlPay.Avalonia"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="350" d:DesignHeight="500"
        x:Class="CtrlPay.Avalonia.LoginWindow"
        xmlns:views="using:CtrlPay.Avalonia.Views"
        xmlns:vm="using:CtrlPay.Avalonia.ViewModels"
        xmlns:avalonia="using:CtrlPay.Avalonia"
        Width="350" Height="500"
        Icon="{DynamicResource Path.AppIcon.ico}"
        WindowStartupLocation="CenterScreen"
        Title="Login to CtrlPay">
    <avalonia:LoginView/>
</Window>
