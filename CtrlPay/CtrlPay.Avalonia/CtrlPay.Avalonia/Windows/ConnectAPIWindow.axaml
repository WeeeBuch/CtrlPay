<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="450" d:DesignHeight="300"
        x:Class="CtrlPay.Avalonia.ConnectAPIWindow"
        xmlns:views="using:CtrlPay.Avalonia.Views"
        xmlns:vm="using:CtrlPay.Avalonia.ViewModels"
        xmlns:avalonia="using:CtrlPay.Avalonia"
        Width="450" Height="300"
        Icon="{DynamicResource Path.AppIcon.ico}"
        WindowStartupLocation="CenterScreen"
        Title="{DynamicResource SettingsView.API.Title}">
    <avalonia:APIConnectView/>
</Window>
